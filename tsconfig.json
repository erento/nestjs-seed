{
  "compilerOptions": {
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strict": true,
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "allowJs": true,
    "pretty": true,
    "baseUrl": "./src",
    "outDir": "./dist"
  },
  "include": [
    "health.ts",
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
