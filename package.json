{
  "name": "x---service-slug---x",
  "version": "1.0.0",
  "description": "x---service-name---x description",
  "license": "ISC",
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erento/x---service-slug---x.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.prod.json",
    "clean-dist": "rimraf dist",
    "lint": "tslint -c tslint.json -e \"**/node_modules/**\" -p ./tsconfig.json",
    "lint:fix": "tslint -c tslint.json --fix -e \"**/node_modules/**\" -p ./tsconfig.json",
    "lint-staged": "lint-staged",
    "clean-pre-prod": "npm prune --production",
    "prebuild": "npm run clean-dist",
    "start": "./dev-env.sh dev-server",
    "start:prod": "echo \"needs to be run in Docker's CMD directly\" && false",
    "test": "./dev-env.sh jest --watch",
    "test:single": "./dev-env.sh jest --ci"
  },
  "dependencies": {
    "@bugsnag/js": "7.3.4",
    "@bugsnag/plugin-express": "7.3.4",
    "@erento/nestjs-common": "1.2.1",
    "@nestjs/common": "7.4.4",
    "@nestjs/core": "7.4.4",
    "@nestjs/platform-express": "7.4.4",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "chalk": "4.1.0",
    "express-http-context": "1.2.4",
    "json-stringify-safe": "5.0.1",
    "moment": "2.28.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "typescript": "3.9.6",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.13",
    "@types/json-stringify-safe": "5.0.0",
    "@types/node": "14.10.1",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "nodemon": "2.0.4",
    "pre-commit": "1.2.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.3.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "tslint-eslint-rules": "5.4.0"
  },
  "precommit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
