  "dependencies": {
    "@bugsnag/js": "7.16.1",
    "@bugsnag/plugin-express": "7.16.1",
    "@erento/eslint-plugin-erento-rules": "1.0.0",
    "@erento/nestjs-common": "file:erento-nestjs-common-4.0.0.tgz",
    "@google-cloud/profiler": "4.1.3",
    "@google-cloud/trace-agent": "5.1.3",
    "@nestjs/common": "8.3.0",
    "@nestjs/core": "8.3.0",
    "@nestjs/platform-express": "8.3.0",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "chalk": "4.1.2",
