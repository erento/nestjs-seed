{
  "name": "x---service-slug---x",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "engineStrict": true,
  "description": "x---service-name---x description",
  "license": "ISC",
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erento/x---service-slug---x.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.prod.json",
    "clean-dist": "rimraf dist",
    "clean-pre-prod": "npm prune --production",
    "lint": "eslint -c .eslintrc.json .",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint --fix -c .eslintrc.json",
    "prebuild": "npm run clean-dist",
    "prepare": "husky install",
    "start": "./dev-env.sh dev-server",
    "start:prod": "echo \"needs to be run in Docker's CMD directly\" && false",
    "test": "./dev-env.sh jest --watch",
    "test:single": "./dev-env.sh jest --ci"
  },
  "dependencies": {
    "@bugsnag/js": "7.18.0",
    "@bugsnag/plugin-express": "7.18.0",
    "@erento/nestjs-common": "7.1.0",
    "@nestjs/axios": "1.0.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.2.1",
    "axios": "1.1.3",
    "body-parser": "1.20.1",
    "express-http-context": "1.2.4",
    "json-stringify-safe": "5.0.1",
    "moment": "2.29.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.4",
    "typescript": "4.6.4",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@erento/eslint-plugin-erento-rules": "2.1.0",
    "@types/jest": "29.2.4",
    "@types/json-stringify-safe": "5.0.0",
    "@types/node": "16.11.41",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.24.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "44.0.2",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
