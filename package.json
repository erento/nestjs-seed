    "@types/node": "16.11.25",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "eslint": "8.6.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.5.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "40.0.0",
    "jest": "27.5.1",
    "lint-staged": "10.5.3",
    "nodemon": "2.0.7",
