{
  "name": "x---service-slug---x",
  "version": "1.0.0",
  "description": "x---service-name---x description",
  "license": "ISC",
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erento/x---service-slug---x.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.prod.json",
    "clean-dist": "rimraf dist",
    "lint": "tslint -c tslint.json -e \"**/node_modules/**\" -p ./tsconfig.json",
    "lint:fix": "tslint -c tslint.json --fix -e \"**/node_modules/**\" -p ./tsconfig.json",
    "lint-staged": "lint-staged",
    "clean-pre-prod": "npm prune --production",
    "start": "./dev-env.sh dev-server",
    "start:prod": "echo \"needs to be run in Docker's CMD directly\" && false",
    "test": "./dev-env.sh jest --watch",
    "test:single": "./dev-env.sh jest --ci"
  },
  "dependencies": {
    "@bugsnag/js": "6.3.2",
    "@bugsnag/plugin-express": "6.3.0",
    "@nestjs/common": "6.4.1",
    "@nestjs/core": "6.4.1",
    "@nestjs/microservices": "6.4.1",
    "@nestjs/platform-express": "6.4.1",
    "@nestjs/testing": "6.4.1",
    "@nestjs/websockets": "6.4.1",
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "chalk": "2.4.2",
    "express-http-context": "1.2.2",
    "json-stringify-safe": "5.0.1",
    "moment": "2.24.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "typescript": "3.5.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/json-stringify-safe": "5.0.0",
    "@types/node": "11.13.17",
    "jest": "24.8.0",
    "lint-staged": "9.0.2",
    "nodemon": "1.19.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-eslint-rules": "5.4.0"
  },
  "precommit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
